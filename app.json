{
  "expo": {
    "name": "Mood Tracker",
    "slug": "mood-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Mood_Tracker_Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Mood_Tracker_Logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.moodtracker.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
